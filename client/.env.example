# Google OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your_client_id                    # From Google Cloud Console OAuth 2.0 Client ID
REACT_APP_OAUTH_REDIRECT_URI=your_oauth_callback_url         # Full URL to your OAuth callback (e.g., https://your-domain.com/oauth-callback)

# Firebase Cloud Functions
REACT_APP_API_URL=your_cloud_function_url                    # Firebase Cloud Function base URL for API endpoints

# Stripe Payment Links (Do not change - configured in Stripe Dashboard)
